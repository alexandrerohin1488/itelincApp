<Page x:Class="intelincApp.MenyPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:intelincApp"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenyPage">

    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center" Width="150">
            <Button Content="Список поставок" x:Name="deliverBtn" Click="deliverBtn_Click" Margin="5"/>
            <Button Content="Список продаж" x:Name="sellerBtn" Click="sellerBtn_Click"  Margin="5"/>
            <Button Content="Склад" x:Name="warehouseBtn" Click="warehouseBtn_Click"  Margin="5"/>
            <Button Content="Назад" x:Name="backBtn" Click="backBtn_Click"  Margin="5"/>
        </StackPanel>
    </Grid>
</Page>
